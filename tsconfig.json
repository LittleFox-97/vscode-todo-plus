{
  "compilerOptions": {
    "target": "ES2024",
    "lib": ["ES2024"],
    "experimentalDecorators": true,
    "rootDir": "src",
    "module": "node20",
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true
  },
  "exclude": [
    "node_modules",
    ".vscode-test"
  ]
}
